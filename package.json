{
  "name": "alyle-ui-demo",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 1111 --no-aot",
    "build": "ng build --target -prod",
    "deploy": "firebase deploy --only \"hosting\"",
    "build:package": "rm -rf .tmp dist build && gulp build:package",
    "publish:package": "yarn build && yarn deploy && yarn build:package && cd build && yarn publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "yarn build:client-and-server-bundles && ts-node server/index"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/platform-server": "^5.1.1",
    "@angular/router": "^5.1.1",
    "@angular/service-worker": "^5.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@types/chroma-js": "^1.3.2",
    "@types/google.analytics": "0.0.36",
    "chroma-js": "^1.3.4",
    "core-js": "^2.4.1",
    "deep-assign": "^2.0.0",
    "hammerjs": "^2.0.8",
    "node-vibrant": "^3.0.0-alpha.2",
    "object-assign-deep": "^0.3.1",
    "prismjs": "^1.6.0",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0-beta.0",
    "@angular/compiler-cli": "^5.1.1",
    "@angular/language-service": "^5.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.26",
    "chalk": "^2.3.0",
    "codelyzer": "4.0.1",
    "del": "^3.0.0",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-rollup": "^2.15.0",
    "gulp-watch": "^4.3.11",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass-tilde-importer": "^1.0.0",
    "protractor": "~5.1.2",
    "rollup": "^0.49.3",
