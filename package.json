{
  "name": "alyle-ui-srcs",
  "version": "2.9.7",
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0 <13.0.0"
  },
  "scripts": {
    "start": "ng serve --port 1111",
    "hmr": "ng serve --port 1111 --hmr",
    "build": "yarn build:browser && yarn build:server && ts-node -O '{\"module\": \"commonjs\"}' server && yarn ngsw-config dist/alyle-ui src/ngsw-config.json",
    "build:browser": "ng build alyle-ui --prod",
    "build:server": "ng run alyle-ui:server",
    "serve": "yarn build && firebase serve only hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mv:index.html": "mkdir functions/dist && mv dist/index.html functions/dist/",
    "build:tools": "rm -rf dist/tools && tsc -p tools",
    "w:tools": "rm -rf dist/tools && tsc -p tools -w",
    "build:@alyle/ui": "yarn ng-packagr -p dist/lib/ng-package.json && yarn build:schematics && node dist/tools/build-lib && node dist/tools/copy-readme",
    "build:universal": "rm -rf dist/server && tsc -p server && yarn build:browser && yarn build:server",
    "serve:universal": "node dist/server",
    "sw:update": "node dist/tools/update-sw",
    "tools:prepare-lib": "node dist/tools/prepare-lib",
    "tools:bump": "node dist/tools/bump-version && yarn changelog",
    "tools:bump-nightly": "node dist/tools/bump-version --nightly",
    "tools:deploy": "node dist/tools/deploy",
    "tools:typedoc": "yarn typedoc --mode modules --json dist/docs.json --tsconfig tsconfig.td.json --excludePrivate --excludeProtected --excludeExternals",
    "tools:docs": "yarn tools:typedoc && node dist/tools/docs-content && node dist/tools/generate-demos",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "serve:lib": "hs dist/docs-content -p 1212 -g --cors 'Access-Control-Allow-Origin' -c0",
    "build:schematics": "tsc -p src/lib/schematics/tsconfig.json && cp -r src/lib/schematics/ dist/@alyle/ui/ && rm -rf dist/@alyle/ui/schematics/**/*.ts",
    "test:sc": "jasmine dist/@alyle/ui/schematics/**/*.spec.js",
    "postinstall": "yarn build:tools"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/elements": "8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "chroma-js": "^2.0.2",
    "core-js": "^2.6.1",
    "document-register-element": "^1.7.2",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "prismjs": "^1.16.0",
    "rxjs": "^6.5.3",
    "tsickle": "^0.37.1",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@alyle/angular-prerender": "^0.4.1",
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-devkit/build-angular": "^0.803.20",
    "@angular-devkit/build-ng-packagr": "^0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@types/chroma-js": "^1.4.1",
    "@types/google.analytics": "^0.0.38",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "~12.12",
    "@types/prismjs": "^1.16.0",
    "@types/showdown": "^1.9.2",
    "@types/yargs": "^13.0.0",
    "ava": "^2.4.0",
    "bundlesize": "^0.17.0",
    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "chokidar": "^3.3.0",
    "codelyzer": "^5.0.0",
    "conventional-changelog-cli": "^2.0.5",
    "firebase-tools": "^7.6.1",
    "fs-extra": "^6.0.0",
    "jasmine-core": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "js-yaml": "^3.11.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "replace-in-file": "^3.4.2",
    "showdown": "^1.9.0",
    "ts-node": "~8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.13.1",
    "typedoc": "^0.15.0",
    "typescript": "~3.5.3",
    "yargs": "^13.2.2"
  },
  "schematics": "./dist/@alyle/ui/schematics/collection.json",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "sources": [
      "src/**/*"
    ],
    "failFast": true,
    "failWithoutAssertions": false
  }
}