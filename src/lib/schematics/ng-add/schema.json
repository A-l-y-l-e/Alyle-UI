{
  "$schema": "http://json-schema.org/schema",
  "id": "alyle-ui-ng-add",
  "title": "Alyle UI ng-add schematic",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "Name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "gestures": {
      "type": "boolean",
      "default": true,
      "description": "Whether gesture support should be set up.",
      "x-prompt": "Set up HammerJS for gesture recognition?"
    }
  },
  "required": []
}