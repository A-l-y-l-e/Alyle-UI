{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "module": "es2015",
    "target": "es5",
    "baseUrl": "../.tmp",
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "inlineSources": false,
    "outDir": "../build",
    "sourceMap": true,
    "rootDir": ".",
    "pretty": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [],
    "paths": {
      "alyle-ui/*": [
        "./*"
      ]
    }
  },
  "angularCompilerOptions": {
    /*"annotateForClosureCompiler": true,*/
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    /*"flatModuleOutFile": "alyle-ui.js",*/
    "flatModuleId": "alyle"
  },
  "files": [
    "./index.ts",
    "./core/index.ts",
    "./icon-button/index.ts",
    "./button/index.ts",
    "./tabs/index.ts",
    "./ripple/index.ts",
    "./radio/index.ts",
    "./ls/index.ts",
    "./drawer/index.ts",
    "./menu/index.ts",
    "./toolbar/index.ts",
    "./svg/index.ts",
    "./responsive/index.ts",
    "./shadow/index.ts",
    "./carousel/index.ts",
    "./input/index.ts",
    "./resizing-cropping-images/index.ts",
    "./date-picker/index.ts"
  ]
}
