{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app-server",
    "types": [
      "node",
    ]
},
  "files": [
    "src/main.server.ts",
    "server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "angularCompilerOptions": {
    "skipLibCheck": true,
    "entryModule": "src/app/app.server.module#AppServerModule"
  }
}
